{
  "name": "vue-mvvm",
  "version": "0.2.0",
  "description": "Lightweight MVVM utilities for Vue 3 with typed ViewModels, UserControls, simple DI and more",
  "keywords": [
    "vue",
    "mvvm",
    "composition-api",
    "vue-mvvm",
    "typescript"
  ],
  "license": "MIT",
  "author": "Christoph Koschel",
  "bugs": "https://github.com/bytelab-studio/vue-mvvm/issues",
  "homepage": "https://iconify-cli.bytelab.studio",
  "repository": "https://github.com/bytelab-studio/vue-mvvm",
  "main": "./dist/vue-mvvm.mjs",
  "types": "./dist/vue-mvvm.d.mts",
  "exports": {
    ".": {
      "import": "./dist/vue-mvvm.mjs",
      "types": "./dist/vue-mvvm.d.mts"
    },
    "./router": {
      "import": "./dist/vue-mvvm-router.mjs",
      "types": "./dist/vue-mvvm-router.d.mts"
    },
    "./dialog": {
      "import": "./dist/vue-mvvm-dialog.mjs",
      "types": "./dist/vue-mvvm-dialog.d.mts"
    }
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:dev": "tsdown --sourcemap --watch",
    "build:prod": "tsdown --minify",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "mermaid": "^11.12.1",
    "tsdown": "^0.16.5",
    "typescript": "~5.9.3",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.17"
  }
}
