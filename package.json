{
    "name": "vue-mvvm",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/vue-mvvm.mjs",
    "types": "./dist/vue-mvvm.d.mts",
    "exports": {
        ".": {
            "import": "./dist/vue-mvvm.mjs",
            "types": "./dist/vue-mvvm.d.mts"
        },
        "./router": {
            "import": "./dist/vue-mvvm-router.mjs",
            "types": "./dist/vue-mvvm-router.d.mts"
        }
    },
    "scripts": {
        "build:dev": "tsdown --entry.vue-mvvm ./src/core/index.ts --entry.vue-mvvm-router ./src/router/index.ts --format esm --dts --watch",
        "dev": "vite",
        "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
        "preview": "vite preview"
    },
    "peerDependencies": {
        "vue": "^3.5.24",
        "vue-router": "^4.6.3"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "tsdown": "^0.16.5",
        "typescript": "~5.9.3"
    }
}
